<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.ProcessRunningController">
    <children>
        <Button layoutX="14.0" layoutY="138.0" mnemonicParsing="false" onAction="#moveToKillProcess" prefHeight="73.0" prefWidth="110.0" text="Kill" />
        <Button layoutX="17.0" layoutY="42.0" mnemonicParsing="false" onAction="#getListRunningProcess" prefHeight="73.0" prefWidth="110.0" text="Get list" />
        <Button layoutX="14.0" layoutY="246.0" mnemonicParsing="false" onAction="#moveToStartProcess" prefHeight="73.0" prefWidth="110.0" text="Start" />
        <TableView fx:id="listProcessTable" layoutX="186.0" layoutY="29.0" prefHeight="310.0" prefWidth="341.0">
            <columns>
                <TableColumn fx:id="nameProcess" prefWidth="243.0" text="Name Process" />
                <TableColumn fx:id="pidProcess" prefWidth="96.0" text="PID" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
